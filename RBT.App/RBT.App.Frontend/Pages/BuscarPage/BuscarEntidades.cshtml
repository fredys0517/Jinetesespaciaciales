@page
@model RBT.App.Frontend.Pages.BuscarPage.BuscarEntidades
@{
    ViewData["Title"] = "BuscarEntidades";
}

<h1>@ViewData["Title"]</h1>

<table class="table">

    @foreach (var auxiliar in Model.ListaAuxiliares)
    {
        <tr>
            <td>@auxiliar.Nombres</td>
            <td>@auxiliar.Apellidos</td>
            <td>@auxiliar.Telefono</td>
            <td>@auxiliar.FechaNacimiento</td>
        </tr>
    }

</table>