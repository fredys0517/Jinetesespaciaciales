@page
@model RBT.App.Frontend.Pages.AuxiliarPage.CrearAuxiliar
@{
    ViewData["AUXILIAR"] = "Auxiliar";
}

<h1>@ViewData["TITULO"]</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="CreateAux.Nombres">Nombres:</label>
        <input asp-for="CreateAux.Nombres" class="form-control">
        <span class="text-danger" asp-validation-for="CreateAux.Nombres"></span>
    </div>
    <div class="form-group">
        <label asp-for="CreateAux.Apellidos">Apellidos:</label>
        <input asp-for="CreateAux.Apellidos" class="form-control">
        <span class="text-danger" asp-validation-for="CreateAux.Apellidos"></span>
    </div>
    <div class="form-group">
        <label asp-for="CreateAux.Telefono">Telefono:</label>
        <input asp-for="CreateAux.Telefono" class="form-control">
        <span class="text-danger" asp-validation-for="CreateAux.Telefono"></span>
    </div>
    <div class="form-group">
        <label asp-for="CreateAux.FechaNacimiento">FechaNacimiento:</label>
        <input asp-for="CreateAux.FechaNacimiento" class="form-control">
        <span class="text-danger" asp-validation-for="CreateAux.FechaNacimiento"></span>
    </div>
    
    <button type="submit" class="btn btn-primary">Crear</button>
</form>
<a asp-page="./ListarAuxiliar" class="btn-default">Lista</a>
  <body style="background-image: url(../../Views/Home/https://p4.wallpaperbetter.com/wallpaper/254/978/507/blue-car-car-repair-workshop-workshop-car-wallpaper-preview.jpg)">

</body>


