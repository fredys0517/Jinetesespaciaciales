@page
@model RBT.App.Frontend.Pages.SoatPage.Soat
@{
    ViewData["Title"] = "Soat";
}

<h1>@ViewData["Title"]</h1>

<div class="text-center">
    <h1 class="text-center">FECHA DE COMPRA</h1>
     <input type="date">
</div>
<div class="text-center">
    <h1 class="text-center">FECHA DE VENCIMIENTO</h1>
     <input type="date">
</div>
<div class="text-center">
    <h1 class="text-center">VEHICULO</h1>
    <input type="number" name="numero">
</div>
<div class="text-center-right">
<button>
<input type="submit" value="Guardar">
</button>
</div>
<div  class="text-center-left">
<button type="reset">
<p><input type="reset" value="Borrar"></p>
</button>
</div>
<div  class="text-center-left">
<p><input type="to update" value="Actualizar"></p>
</button>
</div>